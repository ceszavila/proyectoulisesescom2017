\end{UseCase}

 \begin{UCtrayectoria}
    \UCpaso[\UCactor] Solicita eliminar un residuo sólido oprimiendo el botón \botKo del registro correspondiente en la pantalla \cdtIdRef{IUIBR 1}{Administrar información base para indicadores de residuos sólidos}.
    \UCpaso[\UCsist] Verifica que la escuela se encuentre en estado ``Inscrita''. \refTray{A}.
    \UCpaso[\UCsist] Muestra el mensaje \cdtIdRef{MSG10}{Confirmar la eliminación de un integrante de la línea de acción:} en la pantalla emergente \cdtIdRef{IUIBR 4}{Eliminar residuo sólido} para que el actor confirme la eliminación del residuo sólido.
    \UCpaso[\UCactor] Oprime el botón \cdtButton{Aceptar} de la pantalla emergente \cdtIdRef{IUIBR 4}{Eliminar residuo sólido}. \refTray{B}.
    \UCpaso[\UCsist] Verifica que la escuela se encuentre en estado ``Inscrita''. \refTray{A}.
    \UCpaso[\UCsist] Elimina la información del residuo sólido del sistema.
    \UCpaso[\UCsist] Muestra el mensaje \cdtIdRef{MSG1}{Operación realizada exitosamente} en la pantalla \cdtIdRef{IUIBR 1}{Administrar información base para indicadores de residuos sólidos} para indicar al actor que el registro del residuo sólido se ha eliminado exitosamente. 
    
 \end{UCtrayectoria}
 
 \begin{UCtrayectoriaA}[Fin del caso de uso]{A}{La escuela no se encuentra en un estado que permita eliminar el residuo sólido.}
    \UCpaso[\UCsist] Muestra el mensaje \cdtIdRef{MSG28}{Operación no permitida por estado de la entidad} en la pantalla \cdtIdRef{IUIBR 1}{Administrar información base para indicadores de residuos sólidos} indicando al actor que no puede eliminar el residuo sólido debido a que la escuela no se encuentra en estado ``Inscrita''. 
 \end{UCtrayectoriaA}

    \begin{UCtrayectoriaA}{B}{El actor desea cancelar la eliminación del registro.}
    \UCpaso[\UCactor] Solicita cancelar eliminación del registro del residuo sólido oprimiendo el botón \cdtButton{Cancelar} de la pantalla emergente \cdtIdRef{IUIBR 4}{Eliminar residuo sólido}.
    \UCpaso[] Regresa a la pantalla \cdtIdRef{IUIBR 1}{Administrar información base para indicadores de residuos sólidos}.    
 \end{UCtrayectoriaA}
